{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {
    "name": "fh-mbaas",
    "annotations": {
      "templateVersion": "0.24.45",
      "description": "Red Hat Mobile Backend as a Service template",
      "iconClass": "icon-nodejs"
    }
  },
  "objects": [
    {
      "apiVersion": "v1",
      "kind": "PersistentVolumeClaim",
      "metadata": {
        "name": "mongodb-claim-1"
      },
      "spec": {
        "accessModes": [
          "ReadWriteOnce"
        ],
        "resources": {
          "requests": {
            "storage": "${MONGODB_PVC_SIZE}"
          }
        }
      }
    },
    {
      "kind": "ConfigMap",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-mbaas-info"
      },
      "data": {
        "version": "4.2.0"
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-mbaas-service",
        "labels": {
          "name": "fh-mbaas-service"
        }
      },
      "spec": {
        "ports": [
          {
            "port": 8080
          }
        ],
        "selector": {
          "name": "fh-mbaas"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-messaging-service",
        "labels": {
          "name": "fh-messaging-service"
        }
      },
      "spec": {
        "ports": [
          {
            "port": 8080
          }
        ],
        "selector": {
          "name": "fh-messaging"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-metrics-service",
        "labels": {
          "name": "fh-metrics-service"
        }
      },
      "spec": {
        "ports": [
          {
            "port": 8080
          }
        ],
        "selector": {
          "name": "fh-metrics"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-statsd-service",
        "labels": {
          "name": "fh-statsd-service"
        }
      },
      "spec": {
        "ports": [
          {
            "name": "tcp",
            "protocol": "TCP",
            "port": 8080,
            "targetPort": 8080
          },
          {
            "name": "udp",
            "protocol": "UDP",
            "port": 8081,
            "targetPort": 8081
          }
        ],
        "selector": {
          "name": "fh-statsd"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "mongodb-1",
        "labels": {
          "name": "mongodb-1"
        }
      },
      "spec": {
        "ports": [
          {
            "port": 27017
          }
        ],
        "selector": {
          "name": "mongodb-replica"
        },
        "clusterIP": "None"
      }
    },
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-mbaas",
        "labels": {
          "name": "fh-mbaas"
        }
      },
      "spec": {
        "strategy": {
          "type": "Rolling",
          "resources": {
            "limits": {
              "cpu": "800m",
              "memory": "800Mi"
            },
            "requests": {
              "cpu": "200m",
              "memory": "200Mi"
            }
          }
        },
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "replicas": 1,
        "selector": {
          "name": "fh-mbaas"
        },
        "template": {
          "metadata": {
            "labels": {
              "name": "fh-mbaas"
            }
          },
          "spec": {
            "containers": [
              {
                "name": "fh-mbaas",
                "image": "${FH_MBAAS_IMAGE}:${FH_MBAAS_IMAGE_VERSION}",
                "ports": [
                  {
                    "containerPort": 8080
                  }
                ],
                "env": [
                  {
                    "name": "MBAAS_NAMESPACE",
                    "valueFrom": {
                      "fieldRef": {
                        "fieldPath": "metadata.namespace"
                      }
                    }
                  },
                  {
                    "name": "FH_CLUSTER",
                    "value": "${FH_CLUSTER}"
                  },
                  {
                    "name": "FH_MBAASID",
                    "value": "${FH_MBAASID}"
                  },
                  {
                    "name": "MONGODB_FHMBAAS_USER",
                    "value": "${MONGODB_FHMBAAS_USER}"
                  },
                  {
                    "name": "MONGODB_FHMBAAS_PASSWORD",
                    "value": "${MONGODB_FHMBAAS_PASSWORD}"
                  },
                  {
                    "name": "MONGODB_FHMBAAS_DATABASE",
                    "value": "fh-mbaas"
                  },
                  {
                    "name": "FHMBAAS_KEY",
                    "value": "${FHMBAAS_KEY}"
                  },
                  {
                    "name": "MONGODB_ADMIN_PASSWORD",
                    "value": "${MONGODB_ADMIN_PASSWORD}"
                  },
                  {
                    "name": "FH_METRICS_API_KEY",
                    "description": "API Key for calling fh-metrics",
                    "value": "${FH_METRICS_API_KEY}"
                  },
                  {
                    "name": "FH_MESSAGING_API_KEY",
                    "description": "API Key for calling fh-messaging",
                    "value": "${FH_MESSAGING_API_KEY}"
                  },
                  {
                    "name": "FH_STATSD_API_KEY",
                    "description": "API Key for calling fh-statsd",
                    "value": "${FH_STATSD_API_KEY}"
                  },
                  {
                    "name": "FH_LOG_LEVEL",
                    "value": "${FH_DEFAULT_LOG_LEVEL}"
                  }
                ],
                "resources": {
                  "limits": {
                    "cpu": "800m",
                    "memory": "800Mi"
                  },
                  "requests": {
                    "cpu": "200m",
                    "memory": "200Mi"
                  }
                },
                "imagePullPolicy": "IfNotPresent"
              }
            ]
          }
        }
      }
    },
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-messaging",
        "labels": {
          "name": "fh-messaging"
        }
      },
      "spec": {
        "strategy": {
          "type": "Rolling",
          "resources": {
            "limits": {
              "cpu": "400m",
              "memory": "400Mi"
            },
            "requests": {
              "cpu": "200m",
              "memory": "200Mi"
            }
          }
        },
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "replicas": 1,
        "selector": {
          "name": "fh-messaging"
        },
        "template": {
          "metadata": {
            "labels": {
              "name": "fh-messaging"
            }
          },
          "spec": {
            "containers": [
              {
                "name": "fh-messaging",
                "image": "${FH_MESSAGING_IMAGE}:${FH_MESSAGING_IMAGE_VERSION}",
                "ports": [
                  {
                    "containerPort": 8080
                  }
                ],
                "env": [
                  {
                    "name": "FH_MESSAGING_CRON",
                    "value": "10 0 * * *"
                  },
                  {
                    "name": "MONGODB_FHREPORTING_USER",
                    "value": "${MONGODB_FHREPORTING_USER}"
                  },
                  {
                    "name": "MONGODB_FHREPORTING_PASSWORD",
                    "value": "${MONGODB_FHREPORTING_PASSWORD}"
                  },
                  {
                    "name": "MONGODB_FHREPORTING_DATABASE",
                    "value": "fh-reporting"
                  },
                  {
                    "name": "FH_MESSAGING_API_KEY",
                    "description": "API Key for calling fh-messaging",
                    "value": "${FH_MESSAGING_API_KEY}"
                  },
                  {
                    "name": "FH_LOG_LEVEL",
                    "value": "${FH_DEFAULT_LOG_LEVEL}"
                  }
                ],
                "resources": {
                  "limits": {
                    "cpu": "400m",
                    "memory": "400Mi"
                  },
                  "requests": {
                    "cpu": "200m",
                    "memory": "200Mi"
                  }
                },
                "imagePullPolicy": "IfNotPresent"
              }
            ]
          }
        }
      }
    },
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-metrics",
        "labels": {
          "name": "fh-metrics"
        }
      },
      "spec": {
        "strategy": {
          "type": "Rolling",
          "resources": {
            "limits": {
              "cpu": "400m",
              "memory": "400Mi"
            },
            "requests": {
              "cpu": "200m",
              "memory": "200Mi"
            }
          }
        },
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "replicas": 1,
        "selector": {
          "name": "fh-metrics"
        },
        "template": {
          "metadata": {
            "labels": {
              "name": "fh-metrics"
            }
          },
          "spec": {
            "containers": [
              {
                "name": "fh-metrics",
                "image": "${FH_METRICS_IMAGE}:${FH_METRICS_IMAGE_VERSION}",
                "ports": [
                  {
                    "containerPort": 8080
                  }
                ],
                "env": [
                  {
                    "name": "FH_MBAASID",
                    "value": "${FH_MBAASID}"
                  },
                  {
                    "name": "MONGODB_FHREPORTING_PASSWORD",
                    "value": "${MONGODB_FHREPORTING_PASSWORD}"
                  },
                  {
                    "name": "MONGODB_FHREPORTING_USER",
                    "value": "${MONGODB_FHREPORTING_USER}"
                  },
                  {
                    "name": "MONGODB_FHREPORTING_DATABASE",
                    "value": "fh-reporting"
                  },
                  {
                    "name": "FH_METRICS_API_KEY",
                    "description": "API Key for calling fh-metrics",
                    "value": "${FH_METRICS_API_KEY}"
                  },
                  {
                    "name": "FH_LOG_LEVEL",
                    "value": "${FH_DEFAULT_LOG_LEVEL}"
                  }
                ],
                "resources": {
                  "limits": {
                    "cpu": "400m",
                    "memory": "400Mi"
                  },
                  "requests": {
                    "cpu": "200m",
                    "memory": "200Mi"
                  }
                },
                "imagePullPolicy": "IfNotPresent"
              }
            ]
          }
        }
      }
    },
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-statsd",
        "labels": {
          "name": "fh-statsd"
        }
      },
      "spec": {
        "strategy": {
          "type": "Rolling",
          "resources": {
            "limits": {
              "cpu": "400m",
              "memory": "400Mi"
            },
            "requests": {
              "cpu": "200m",
              "memory": "200Mi"
            }
          }
        },
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "replicas": 1,
        "selector": {
          "name": "fh-statsd"
        },
        "template": {
          "metadata": {
            "labels": {
              "name": "fh-statsd"
            }
          },
          "spec": {
            "containers": [
              {
                "name": "fh-statsd",
                "image": "${FH_STATSD_IMAGE}:${FH_STATSD_IMAGE_VERSION}",
                "ports": [
                  {
                    "containerPort": 8080
                  }
                ],
                "env": [
                  {
                    "name": "FH_STATSD_API_KEY",
                    "description": "API Key for calling fh-statsd",
                    "value": "${FH_STATSD_API_KEY}"
                  },
                  {
                    "name": "FH_LOG_LEVEL",
                    "value": "${FH_DEFAULT_LOG_LEVEL}"
                  }
                ],
                "resources": {
                  "limits": {
                    "cpu": "400m",
                    "memory": "400Mi"
                  },
                  "requests": {
                    "cpu": "200m",
                    "memory": "200Mi"
                  }
                },
                "imagePullPolicy": "IfNotPresent"
              }
            ]
          }
        }
      }
    },
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "mongodb-1",
        "labels": {
          "name": "mongodb"
        }
      },
      "spec": {
        "strategy": {
          "type": "Recreate",
          "resources": {
            "limits": {
              "cpu": "1000m",
              "memory": "1000Mi"
            },
            "requests": {
              "cpu": "200m",
              "memory": "200Mi"
            }
          }
        },
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "replicas": 1,
        "selector": {
          "name": "mongodb-replica"
        },
        "template": {
          "metadata": {
            "labels": {
              "name": "mongodb-replica"
            }
          },
          "spec": {
            "volumes": [
              {
                "name": "mongodb-data-volume",
                "persistentVolumeClaim": {
                  "claimName": "mongodb-claim-1"
                }
              }
            ],
            "containers": [
              {
                "name": "mongodb-1",
                "image": "${MONGODB_IMAGE}:${MONGODB_IMAGE_VERSION}",
                "ports": [
                  {
                    "containerPort": 27017
                  }
                ],
                "env": [
                  {
                    "name": "MONGODB_SERVICE_NAME",
                    "value": "mongodb"
                  },
                  {
                    "name": "MONGODB_KEYFILE_VALUE",
                    "value": "${MONGODB_KEYFILE_VALUE}"
                  },
                  {
                    "name": "MONGODB_FHMBAAS_USER",
                    "value": "${MONGODB_FHMBAAS_USER}"
                  },
                  {
                    "name": "MONGODB_FHMBAAS_PASSWORD",
                    "value": "${MONGODB_FHMBAAS_PASSWORD}"
                  },
                  {
                    "name": "MONGODB_FHMBAAS_DATABASE",
                    "value": "fh-mbaas"
                  },
                  {
                    "name": "FHMBAAS_KEY",
                    "value": "${FHMBAAS_KEY}"
                  },
                  {
                    "name": "MONGODB_ADMIN_PASSWORD",
                    "value": "${MONGODB_ADMIN_PASSWORD}"
                  },
                  {
                    "name": "MONGODB_FHSTATSD_USER",
                    "value": "${MONGODB_FHSTATSD_USER}"
                  },
                  {
                    "name": "MONGODB_FHSTATSD_PASSWORD",
                    "value": "${MONGODB_FHSTATSD_PASSWORD}"
                  },
                  {
                    "name": "MONGODB_FHSTATSD_DATABASE",
                    "value": "fh-statsd"
                  },
                  {
                    "name": "MONGODB_FHREPORTING_USER",
                    "value": "${MONGODB_FHREPORTING_USER}"
                  },
                  {
                    "name": "MONGODB_FHREPORTING_PASSWORD",
                    "value": "${MONGODB_FHREPORTING_PASSWORD}"
                  },
                  {
                    "name": "MONGODB_FHREPORTING_DATABASE",
                    "value": "fh-reporting"
                  }
                ],
                "resources": {
                  "limits": {
                    "cpu": "1000m",
                    "memory": "1000Mi"
                  },
                  "requests": {
                    "cpu": "200m",
                    "memory": "200Mi"
                  }
                },
                "volumeMounts": [
                  {
                    "name": "mongodb-data-volume",
                    "mountPath": "/var/lib/mongodb/data"
                  }
                ],
                "imagePullPolicy": "IfNotPresent"
              }
            ]
          }
        }
      }
    },
    {
      "kind": "Route",
      "apiVersion": "v1",
      "metadata": {
        "name": "mbaas",
        "annotations": {
          "openshift.io/host.generated": "false"
        }
      },
      "spec": {
        "host": "${MBAAS_ROUTER_DNS}",
        "to": {
          "kind": "Service",
          "name": "fh-mbaas-service"
        },
        "tls": {
          "termination": "edge",
          "insecureEdgeTerminationPolicy": "Allow"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "nagios",
        "labels": {
          "name": "nagios"
        },
        "creationTimestamp": null
      },
      "spec": {
        "ports": [
          {
            "protocol": "TCP",
            "port": 8080,
            "targetPort": 8080
          }
        ],
        "selector": {
          "name": "nagios"
        },
        "type": "ClusterIP",
        "sessionAffinity": "None"
      },
      "status": {
        "loadBalancer": {}
      }
    },
    {
      "kind": "ServiceAccount",
      "apiVersion": "v1",
      "metadata": {
        "name": "nagios"
      }
    },
    {
      "kind": "RoleBinding",
      "apiVersion": "v1",
      "metadata": {
        "name": "nagiosadmin"
      },
      "subjects": [
        {
          "kind": "ServiceAccount",
          "name": "nagios"
        }
      ],
      "roleRef": {
        "name": "admin"
      }
    },
    {
      "kind": "PersistentVolumeClaim",
      "apiVersion": "v1",
      "metadata": {
        "name": "nagios-claim-1"
      },
      "spec": {
        "accessModes": [
          "ReadWriteOnce"
        ],
        "resources": {
          "requests": {
            "storage": "1Gi"
          }
        }
      }
    },
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "nagios",
        "creationTimestamp": null
      },
      "spec": {
        "strategy": {
          "type": "Recreate",
          "recreateParams": {
            "timeoutSeconds": 600
          },
          "resources": {}
        },
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "replicas": 1,
        "test": false,
        "selector": {
          "name": "nagios"
        },
        "template": {
          "metadata": {
            "creationTimestamp": null,
            "labels": {
              "name": "nagios"
            }
          },
          "spec": {
            "volumes": [
              {
                "name": "nagios-data-volume",
                "persistentVolumeClaim": {
                  "claimName": "nagios-claim-1"
                }
              }
            ],
            "containers": [
              {
                "name": "nagios",
                "image": "${NAGIOS_IMAGE}:${NAGIOS_IMAGE_VERSION}",
                "ports": [
                  {
                    "containerPort": 8080,
                    "protocol": "TCP"
                  }
                ],
                "volumeMounts": [
                  {
                    "name": "nagios-data-volume",
                    "mountPath": "/var/log/nagios"
                  }
                ],
                "env": [
                  {
                    "name": "SMTP_SERVER",
                    "value": "${SMTP_SERVER}"
                  },
                  {
                    "name": "SMTP_USERNAME",
                    "value": "${SMTP_USERNAME}"
                  },
                  {
                    "name": "SMTP_PASSWORD",
                    "value": "${SMTP_PASSWORD}"
                  },
                  {
                    "name": "SMTP_TLS",
                    "value": "${SMTP_TLS}"
                  },
                  {
                    "name": "SMTP_FROM_ADDRESS",
                    "value": "${SMTP_FROM_ADDRESS}"
                  },
                  {
                    "name": "ADMIN_EMAIL",
                    "value": "${ADMIN_EMAIL}"
                  },
                  {
                    "name": "NAGIOS_USER",
                    "value": "${NAGIOS_USER}"
                  },
                  {
                    "name": "NAGIOS_PASSWORD",
                    "value": "${NAGIOS_PASSWORD}"
                  },
                  {
                    "name": "RHMAP_ROUTER_DNS",
                    "value": "${RHMAP_ROUTER_DNS}"
                  },
                  {
                    "name": "RHMAP_HOSTGROUPS",
                    "value": "${RHMAP_HOSTGROUPS}"
                  }
                ],
                "resources": {
                  "limits": {
                    "cpu": "800m",
                    "memory": "800Mi"
                  },
                  "requests": {
                    "cpu": "200m",
                    "memory": "200Mi"
                  }
                },
                "terminationMessagePath": "/dev/termination-log",
                "imagePullPolicy": "IfNotPresent"
              }
            ],
            "restartPolicy": "Always",
            "terminationGracePeriodSeconds": 30,
            "dnsPolicy": "ClusterFirst",
            "serviceAccountName": "nagios",
            "serviceAccount": "nagios",
            "securityContext": {}
          }
        }
      },
      "status": {}
    },
    {
      "kind": "Route",
      "apiVersion": "v1",
      "metadata": {
        "name": "nagios",
        "creationTimestamp": null
      },
      "spec": {
        "host": "",
        "to": {
          "kind": "Service",
          "name": "nagios"
        },
        "tls": {
          "termination": "edge",
          "insecureEdgeTerminationPolicy": "Allow"
        }
      },
      "status": {
        "ingress": null
      }
    }
  ],
  "parameters": [
    {
      "name": "FH_CLUSTER",
      "value": "development",
      "description": "the name of the cluster the mbaas was created from"
    },
    {
      "name": "FH_MBAASID",
      "value": "default",
      "description": "id of the mbaas. A value chosen by the user"
    },
    {
      "name": "MONGODB_FHMBAAS_USER",
      "value": "mbaas-user",
      "description": "The MongoDB user name"
    },
    {
      "name": "MONGODB_FHMBAAS_PASSWORD",
      "description": "The MongoDB password",
      "generate": "expression",
      "from": "[a-zA-Z0-9]{40}"
    },
    {
      "name": "FHMBAAS_KEY",
      "description": "The MBaaS key",
      "generate": "expression",
      "from": "[a-zA-Z0-9]{40}"
    },
    {
      "name": "MONGODB_KEYFILE_VALUE",
      "description": "The value of the MongoDB Key",
      "generate": "expression",
      "from": "[a-zA-Z0-9]{255}"
    },
    {
      "name": "MONGODB_ADMIN_PASSWORD",
      "description": "The MongoDB Admin password",
      "generate": "expression",
      "from": "[a-zA-Z0-9]{40}"
    },
    {
      "name": "MONGODB_FHREPORTING_USER",
      "value": "reporting-user",
      "description": "The Reporting user name"
    },
    {
      "name": "MONGODB_FHREPORTING_PASSWORD",
      "description": "The MongoDB Reporting password",
      "generate": "expression",
      "from": "[a-zA-Z0-9]{40}"
    },
    {
      "name": "FH_MBAAS_IMAGE",
      "value": "docker.io/feedhenry/fh-mbaas",
      "description": "The name of the fh-mbaas image"
    },
    {
      "name": "FH_MBAAS_IMAGE_VERSION",
      "value": "5.3.15-1276",
      "description": "The version for the FH_MBAAS_IMAGE"
    },
    {
      "name": "FH_MESSAGING_IMAGE",
      "value": "docker.io/feedhenry/fh-messaging",
      "description": "The name of the fh-messaging image"
    },
    {
      "name": "FH_MESSAGING_IMAGE_VERSION",
      "value": "3.0.4-479",
      "description": "The version for the FH_MESSAGING_IMAGE"
    },
    {
      "name": "FH_METRICS_IMAGE",
      "value": "docker.io/feedhenry/fh-metrics",
      "description": "The name of the fh-metrics image"
    },
    {
      "name": "FH_METRICS_IMAGE_VERSION",
      "value": "3.0.4-479",
      "description": "The version for the FH_METRICS_IMAGE"
    },
    {
      "name": "FH_STATSD_IMAGE",
      "value": "docker.io/feedhenry/fh-statsd",
      "description": "The name of the fh-statsd image"
    },
    {
