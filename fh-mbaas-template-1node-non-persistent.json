{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {
    "name": "fh-mbaas",
    "annotations": {
      "templateVersion": "0.26.79",
      "description": "Red Hat Mobile Backend as a Service template",
      "iconClass": "icon-nodejs"
    }
  },
  "objects": [
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-mbaas-service",
        "labels": {
          "name": "fh-mbaas-service"
        },
        "annotations": {
          "service.alpha.openshift.io/dependencies": "[{\"name\":\"mongodb-1\",\"namespace\":\"\",\"kind\":\"Service\"},{\"name\":\"fh-messaging-service\",\"namespace\":\"\",\"kind\":\"Service\"},{\"name\":\"fh-metrics-service\",\"namespace\":\"\",\"kind\":\"Service\"},{\"name\":\"fh-statsd-service\",\"namespace\":\"\",\"kind\":\"Service\"}]"
        }
      },
      "spec": {
        "ports": [
          {
            "port": 8080
          }
        ],
        "selector": {
          "name": "fh-mbaas"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-messaging-service",
        "labels": {
          "name": "fh-messaging-service"
        }
      },
      "spec": {
        "ports": [
          {
            "port": 8080
          }
        ],
        "selector": {
          "name": "fh-messaging"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-metrics-service",
        "labels": {
          "name": "fh-metrics-service"
        }
      },
      "spec": {
        "ports": [
          {
            "port": 8080
          }
        ],
        "selector": {
          "name": "fh-metrics"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-statsd-service",
        "labels": {
          "name": "fh-statsd-service"
        }
      },
      "spec": {
        "ports": [
          {
            "name": "tcp",
            "protocol": "TCP",
            "port": 8080,
            "targetPort": 8080
          },
          {
            "name": "udp",
            "protocol": "UDP",
            "port": 8081,
            "targetPort": 8081
          }
        ],
        "selector": {
          "name": "fh-statsd"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "mongodb-1",
        "labels": {
          "name": "mongodb-1"
        }
      },
      "spec": {
        "ports": [
          {
            "port": 27017
          }
        ],
        "selector": {
          "name": "mongodb-replica-1"
        },
        "clusterIP": "None"
      }
    },
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-mbaas",
        "labels": {
          "name": "fh-mbaas"
        }
      },
      "spec": {
        "strategy": {
          "type": "Rolling",
          "resources": {
            "limits": {
              "cpu": "800m",
              "memory": "800Mi"
            },
            "requests": {
              "cpu": "200m",
              "memory": "200Mi"
            }
          }
        },
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "replicas": "${FH_MBAAS_REPLICAS}",
        "selector": {
          "name": "fh-mbaas"
        },
        "template": {
          "metadata": {
            "labels": {
              "name": "fh-mbaas"
            }
          },
          "spec": {
            "containers": [
              {
                "name": "fh-mbaas",
                "image": "${FH_MBAAS_IMAGE}:${FH_MBAAS_IMAGE_VERSION}",
                "ports": [
                  {
                    "containerPort": 8080
                  }
                ],
                "env": [
                  {
                    "name": "MBAAS_NAMESPACE",
                    "valueFrom": {
                      "fieldRef": {
                        "fieldPath": "metadata.namespace"
                      }
                    }
                  },
                  {
                    "name": "FH_CLUSTER",
                    "value": "${FH_CLUSTER}"
                  },
                  {
                    "name": "FH_MBAASID",
                    "value": "${FH_MBAASID}"
                  },
                  {
                    "name": "MONGODB_REPLICA_NAME",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-replica-name"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_FHMBAAS_USER",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-fhmbaas-user"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_FHMBAAS_PASSWORD",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-fhmbaas-password"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_FHMBAAS_DATABASE",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-fhmbaas-database"
                      }
                    }
                  },
                  {
                    "name": "FHMBAAS_KEY",
                    "value": "${FHMBAAS_KEY}"
                  },
                  {
                    "name": "MONGODB_ADMIN_PASSWORD",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-admin-password"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_FORM_PASSWORD",
                    "description": "Password to create fh-forms user in Mongo",
                    "value": "${MONGODB_FORM_PASSWORD}"
                  },
                  {
                    "name": "FH_METRICS_API_KEY",
                    "description": "API Key for calling fh-metrics",
                    "value": "${FH_METRICS_API_KEY}"
                  },
                  {
                    "name": "FH_MESSAGING_API_KEY",
                    "description": "API Key for calling fh-messaging",
                    "value": "${FH_MESSAGING_API_KEY}"
                  },
                  {
                    "name": "FH_STATSD_API_KEY",
                    "description": "API Key for calling fh-statsd",
                    "value": "${FH_STATSD_API_KEY}"
                  },
                  {
                    "name": "FH_LOG_LEVEL",
                    "value": "${FH_DEFAULT_LOG_LEVEL}"
                  },
                  {
                    "name": "MONGODB_REPLICA_NAME",
                    "value": "${MONGODB_REPLICA_NAME}"
                  },
                  {
                    "name": "HTTPS_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "https-proxy"
                      }
                    }
                  },
                  {
                    "name": "HTTP_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "http-proxy"
                      }
                    }
                  },
                  {
                    "name": "NO_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "no-proxy"
                      }
                    }
                  }
                ],
                "resources": {
                  "limits": {
                    "cpu": "800m",
                    "memory": "800Mi"
                  },
                  "requests": {
                    "cpu": "200m",
                    "memory": "200Mi"
                  }
                },
                "livenessProbe": {
                  "httpGet": {
                    "path": "/sys/info/ping",
                    "port": 8080,
                    "scheme": "HTTP"
                  },
                  "initialDelaySeconds": 55,
                  "timeoutSeconds": 5,
                  "periodSeconds": 60,
                  "successThreshold": 1,
                  "failureThreshold": 2
                },
                "readinessProbe": {
                  "httpGet": {
                    "path": "/sys/info/ping",
                    "port": 8080,
                    "scheme": "HTTP"
                  },
                  "timeoutSeconds": 5,
                  "periodSeconds": 10,
                  "successThreshold": 1,
                  "failureThreshold": 1
                },
                "imagePullPolicy": "IfNotPresent"
              }
            ],
            "volumes": [
              {
                "name": "node-proxy",
                "configMap": {
                  "name": "node-proxy"
                }
              }
            ]
          }
        }
      }
    },
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-messaging",
        "labels": {
          "name": "fh-messaging"
        }
      },
      "spec": {
        "strategy": {
          "type": "Rolling",
          "resources": {
            "limits": {
              "cpu": "400m",
              "memory": "400Mi"
            },
            "requests": {
              "cpu": "200m",
              "memory": "200Mi"
            }
          }
        },
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "replicas": "${FH_MESSAGING_REPLICAS}",
        "selector": {
          "name": "fh-messaging"
        },
        "template": {
          "metadata": {
            "labels": {
              "name": "fh-messaging"
            }
          },
          "spec": {
            "containers": [
              {
                "name": "fh-messaging",
                "image": "${FH_MESSAGING_IMAGE}:${FH_MESSAGING_IMAGE_VERSION}",
                "ports": [
                  {
                    "containerPort": 8080
                  }
                ],
                "env": [
                  {
                    "name": "FH_MESSAGING_CRON",
                    "value": "10 0 * * *"
                  },
                  {
                    "name": "MONGODB_FHREPORTING_USER",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-fhreporting-user"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_FHREPORTING_PASSWORD",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-fhreporting-password"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_FHREPORTING_DATABASE",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-fhreporting-database"
                      }
                    }
                  },
                  {
                    "name": "FH_MESSAGING_API_KEY",
                    "description": "API Key for calling fh-messaging",
                    "value": "${FH_MESSAGING_API_KEY}"
                  },
                  {
                    "name": "FH_LOG_LEVEL",
                    "value": "${FH_DEFAULT_LOG_LEVEL}"
                  },
                  {
                    "name": "HTTPS_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "https-proxy"
                      }
                    }
                  },
                  {
                    "name": "HTTP_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "http-proxy"
                      }
                    }
                  },
                  {
                    "name": "NO_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "no-proxy"
                      }
                    }
                  }
                ],
                "resources": {
                  "limits": {
                    "cpu": "400m",
                    "memory": "400Mi"
                  },
                  "requests": {
                    "cpu": "200m",
                    "memory": "200Mi"
                  }
                },
                "livenessProbe": {
                  "httpGet": {
                    "path": "/sys/info/ping",
                    "port": 8080,
                    "scheme": "HTTP"
                  },
                  "initialDelaySeconds": 55,
                  "timeoutSeconds": 5,
                  "periodSeconds": 60,
                  "successThreshold": 1,
                  "failureThreshold": 2
                },
                "readinessProbe": {
                  "httpGet": {
                    "path": "/sys/info/ping",
                    "port": 8080,
                    "scheme": "HTTP"
                  },
                  "timeoutSeconds": 5,
                  "periodSeconds": 10,
                  "successThreshold": 1,
                  "failureThreshold": 1
                },
                "imagePullPolicy": "IfNotPresent"
              }
            ],
            "volumes": [
              {
                "name": "node-proxy",
                "configMap": {
                  "name": "node-proxy"
                }
              }
            ]
          }
        }
      }
    },
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-metrics",
        "labels": {
          "name": "fh-metrics"
        }
      },
      "spec": {
        "strategy": {
          "type": "Rolling",
          "resources": {
            "limits": {
              "cpu": "400m",
              "memory": "400Mi"
            },
            "requests": {
              "cpu": "200m",
              "memory": "200Mi"
            }
          }
        },
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "replicas": "${FH_METRICS_REPLICAS}",
        "selector": {
          "name": "fh-metrics"
        },
        "template": {
          "metadata": {
            "labels": {
              "name": "fh-metrics"
            }
          },
          "spec": {
            "containers": [
              {
                "name": "fh-metrics",
                "image": "${FH_METRICS_IMAGE}:${FH_METRICS_IMAGE_VERSION}",
                "ports": [
                  {
                    "containerPort": 8080
                  }
                ],
                "env": [
                  {
                    "name": "FH_MBAASID",
                    "value": "${FH_MBAASID}"
                  },
                  {
                    "name": "MONGODB_FHREPORTING_USER",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-fhreporting-user"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_FHREPORTING_PASSWORD",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-fhreporting-password"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_FHREPORTING_DATABASE",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-fhreporting-database"
                      }
                    }
                  },
                  {
                    "name": "FH_METRICS_API_KEY",
                    "description": "API Key for calling fh-metrics",
                    "value": "${FH_METRICS_API_KEY}"
                  },
                  {
                    "name": "FH_LOG_LEVEL",
                    "value": "${FH_DEFAULT_LOG_LEVEL}"
                  },
                  {
                    "name": "HTTPS_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "https-proxy"
                      }
                    }
                  },
                  {
                    "name": "HTTP_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "http-proxy"
                      }
                    }
                  },
                  {
                    "name": "NO_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "no-proxy"
                      }
                    }
                  }
                ],
                "resources": {
                  "limits": {
                    "cpu": "400m",
                    "memory": "400Mi"
                  },
                  "requests": {
                    "cpu": "200m",
                    "memory": "200Mi"
                  }
                },
                "livenessProbe": {
                  "httpGet": {
                    "path": "/sys/info/ping",
                    "port": 8080,
                    "scheme": "HTTP"
                  },
                  "initialDelaySeconds": 55,
                  "timeoutSeconds": 5,
                  "periodSeconds": 60,
                  "successThreshold": 1,
                  "failureThreshold": 2
                },
                "readinessProbe": {
                  "httpGet": {
                    "path": "/sys/info/ping",
                    "port": 8080,
                    "scheme": "HTTP"
                  },
                  "timeoutSeconds": 5,
                  "periodSeconds": 10,
                  "successThreshold": 1,
                  "failureThreshold": 1
                },
                "imagePullPolicy": "IfNotPresent"
              }
            ],
            "volumes": [
              {
                "name": "node-proxy",
                "configMap": {
                  "name": "node-proxy"
                }
              }
            ]
          }
        }
      }
    },
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "fh-statsd",
        "labels": {
          "name": "fh-statsd"
        }
      },
      "spec": {
        "strategy": {
          "type": "Rolling",
          "resources": {
            "limits": {
              "cpu": "400m",
              "memory": "400Mi"
            },
            "requests": {
              "cpu": "200m",
              "memory": "200Mi"
            }
          }
        },
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "replicas": 1,
        "selector": {
          "name": "fh-statsd"
        },
        "template": {
          "metadata": {
            "labels": {
              "name": "fh-statsd"
            }
          },
          "spec": {
            "containers": [
              {
                "name": "fh-statsd",
                "image": "${FH_STATSD_IMAGE}:${FH_STATSD_IMAGE_VERSION}",
                "ports": [
                  {
                    "containerPort": 8080
                  }
                ],
                "env": [
                  {
                    "name": "FH_STATSD_API_KEY",
                    "description": "API Key for calling fh-statsd",
                    "value": "${FH_STATSD_API_KEY}"
                  },
                  {
                    "name": "FH_LOG_LEVEL",
                    "value": "${FH_DEFAULT_LOG_LEVEL}"
                  },
                  {
                    "name": "HTTPS_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "https-proxy"
                      }
                    }
                  },
                  {
                    "name": "HTTP_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "http-proxy"
                      }
                    }
                  },
                  {
                    "name": "NO_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "no-proxy"
                      }
                    }
                  }
                ],
                "resources": {
                  "limits": {
                    "cpu": "400m",
                    "memory": "400Mi"
                  },
                  "requests": {
                    "cpu": "200m",
                    "memory": "200Mi"
                  }
                },
                "livenessProbe": {
                  "httpGet": {
                    "path": "/sys/info/ping",
                    "port": 8080,
                    "scheme": "HTTP"
                  },
                  "initialDelaySeconds": 55,
                  "timeoutSeconds": 5,
                  "periodSeconds": 60,
                  "successThreshold": 1,
                  "failureThreshold": 2
                },
                "readinessProbe": {
                  "httpGet": {
                    "path": "/sys/info/ping",
                    "port": 8080,
                    "scheme": "HTTP"
                  },
                  "timeoutSeconds": 5,
                  "periodSeconds": 10,
                  "successThreshold": 1,
                  "failureThreshold": 1
                },
                "imagePullPolicy": "IfNotPresent"
              }
            ],
            "volumes": [
